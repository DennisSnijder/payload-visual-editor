{
  "name": "payload-visual-editor",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:pemedia/payload-visual-editor.git",
  "license": "MIT",
  "scripts": {
    "shell": "docker exec -i -t payload-visual-editor-node-1 /bin/sh",
    "local:build-watch": "tsc -w",
    "plugin:yarn": "docker exec -t payload-visual-editor-node-1 /bin/sh -c 'yarn'",
    "plugin:build-watch": "docker exec -t payload-visual-editor-node-1 /bin/sh -c 'yarn tsc -w'",
    "example:cms:yarn": "docker exec -t payload-visual-editor-node-1 /bin/sh -c 'cd example/cms && yarn'",
    "example:cms:dev": "docker exec -t payload-visual-editor-node-1 /bin/sh -c 'cd example/cms && yarn dev'",
    "example:cms:generate-types": "docker exec -t payload-visual-editor-node-1 /bin/sh -c 'cd example/cms && yarn generate:types'",
    "example:website:yarn": "docker exec -t payload-visual-editor-node-1 /bin/sh -c 'cd example/website && yarn'",
    "example:website:dev": "docker exec -t payload-visual-editor-node-1 /bin/sh -c 'cd example/website && yarn dev'"
  },
  "peerDependencies": {
    "payload": "^1.9.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.9",
    "payload": "^1.9.0",
    "react": "^18.2.0",
    "typescript": "^5.1.3"
  }
}
